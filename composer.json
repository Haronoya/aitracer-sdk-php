{
    "name": "haro/aitracer",
    "description": "AITracer PHP SDK - Monitor and trace AI/LLM API calls",
    "version": "1.3.0",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "ai",
        "llm",
        "monitoring",
        "tracing",
        "openai",
        "anthropic",
        "gpt",
        "claude",
        "observability"
    ],
    "authors": [
        {
            "name": "AITracer",
            "email": "support@aitracer.co"
        }
    ],
    "homepage": "https://aitracer.co",
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "AITracer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AITracer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-fix": "php-cs-fixer fix",
        "analyse": "phpstan analyse src tests"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
